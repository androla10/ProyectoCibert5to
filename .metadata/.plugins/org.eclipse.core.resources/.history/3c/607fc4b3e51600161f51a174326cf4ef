	        <id column="iTipo" property="idTipoUsuario" jdbcType="INTEGER"/>
	        <result column="sDescripcion" property="TipoUsuario" jdbcType="VARCHAR"/>
	        <result column="sClave" property="password" jdbcType="VARCHAR"/>
	        <result column="iTipo" property="idTipo" jdbcType="INTEGER"/>
	        <result column="iActivo" property="idActivo" jdbcType="INTEGER"/>
	    </resultMap>

		<select id="listarCombo" resultMap="rmListarTipoUsuario">
		    	SELECT 		
