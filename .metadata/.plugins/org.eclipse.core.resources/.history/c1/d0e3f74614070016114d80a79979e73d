$(document).ready(function() {
//	$(".list-group").children().css("color", "blue");
	$(".list-group").children("a").each(function(index,element){
		alert($(this).val() + " : "+ $(this).text() +" : "+$(element).html());
		$(element).html("<i class='fa fa-cog fa-fw'> </i> Hola")
//		$(this).html("<i>HOLA!</i>");
	});
//	alert();
});